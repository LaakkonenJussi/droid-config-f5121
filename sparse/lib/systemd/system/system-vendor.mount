[Unit]
Description=Mount SW binaries that have been flashed to the cache partition
Before=local-fs.target systemd-modules-load.service

[Mount]
What=/dev/mmcblk0p24
Where=/system/vendor
Type=ext4
Options=ro,nosuid,nodev,barrier=1,data=ordered,nodelalloc,nomblk_io_submit,errors=panic
TimeoutSec=10

[Install]
WantedBy=local-fs.target

